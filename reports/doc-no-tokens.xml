<odoo>
    <data>

        <template id="report_document">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="doc">
                    <t t-call="web.external_layout">
                        <div class="page">
                            <span t-field="doc.body"/>
                        </div>
                    </t>
                </t>
            </t>
        </template>

    </data>
</odoo>

