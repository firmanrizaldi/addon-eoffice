<odoo>
	<data>

		<record id="document_sequence" model="ir.sequence">
			<field name="name">document sequence</field>
			<field name="code">eo.doc</field>
			<field name="prefix">UMJ/%(year)s/%(month)s//</field>
			<field name="padding">3</field>
			<field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
		</record>
		
	</data>
</odoo>